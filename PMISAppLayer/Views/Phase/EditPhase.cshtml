@using PMISBLayer.Entities
@using PMISBLayer.DTOs
@model Phase

<form action="/Phase/UpdatePhase">
    @Html.HiddenFor(x => x.PhaseId)
    <div class="form-group">
        <label>Phase Name</label>
        <input class="form-control" asp-for="PhaseName" />
    </div>

    <input class="btn btn-outline-primary btn-lg btn-block" type="submit" value="Update" />
    <a class="btn btn-outline-secondary btn-lg btn-block" href="/Phase/Index">Back</a>
</form>


